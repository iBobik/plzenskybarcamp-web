<h2>Users</h2>
<table>
	<tr>
		<th>User name</th>
		<th>Twitter</th>
		<th>Facebook</th>
		<th>LinkedIn</th>
		<th>Bio</th>
	</tr>

	<tr n:foreach="$users as $user">
		<td>{$user['name']}</td>
		<td>{$user['twitter']}</td>
		<td>{$user['facebook']}</td>
		<td>{$user['linked']}</td>
		<td>{$user['bio']}</td>
	</tr>
</table>